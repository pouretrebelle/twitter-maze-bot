{
  "name": "twitter-mazes",
  "version": "0.0.1",
  "description": "Posts mazes on twitter and tracks replies",
  "main": "server.js",
  "scripts": {
    "start": "nodemon src/index.js --exec babel-node --presets es2015 --plugins transform-class-properties",
    "build": "babel src -d dist --presets es2015 --plugins transform-class-properties",
    "serve": "node dist/index.js",
    "forever": "forever start dist/index.js",
    "lint": "eslint src"
  },
  "dependencies": {
    "canvas": "^1.6.5",
    "deasync": "^0.1.9",
    "dotenv": "^4.0.0",
    "forever": "^0.15.3",
    "twitter": "^1.7.0"
  },
  "engines": {
    "node": "6.9.1"
  },
  "repository": {
    "url": ""
  },
  "keywords": [
    "node",
    "twitter"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "nodemon": "^1.11.0"
  }
}
